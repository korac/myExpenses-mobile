{
  "name": "myexpenses-mobile",
  "version": "0.0.1",
  "private": true,
  "description": "MyExpenses react native app.",
  "author": "MyExpenses <korac.kristijan@gmail.com>",
  "main": "node_modules/expo/AppEntry.js",
  "dependencies": {
    "expo": "^31.0.2",
    "react": "16.5.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-31.0.0.tar.gz",
    "react-navigation": "^3.0.9"
  },
  "devDependencies": {
    "babel-preset-expo": "^5.0.0",
    "eslint": "^5.11.1",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "prettier": "^1.15.3",
    "pretty-quick": "^1.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "lint": "eslint --quiet --fix src/",
    "dist:android": "npm version patch && npm run build:android",
    "dist:android:sprint": "npm version minor && npm run build:android",
    "build:android": "expo build:android"
  }
}
