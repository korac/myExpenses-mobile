{
  "expo": {
    "name": "MyExpenses",
    "slug": "myExpenses-mobile",
    "icon": "./assets/images/icon.png",
    "version": "1.0.0",
    "privacy": "public",
    "githubUrl": "https://github.com/korac/myExpenses-mobile",
    "sdkVersion": "31.0.0",
    "entryPoint": "./components/App/index",
    "platforms": [
      "ios",
      "android"
    ],
    "orientation": "portrait",
    "primaryColor": "#633147",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#633147"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mobile.myexpenses"
    },
    "android": {
      "package": "com.mobile.myexpenses"
    }
  }
}